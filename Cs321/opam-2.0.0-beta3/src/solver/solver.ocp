if build_libs = [] ||
   %mem( string = "opam-solver"
         strings = build_libs )
then {
begin library "opam-solver"

  files   = [
    "opamSolverConfig.ml"
    "opamActionGraph.ml"
    "opamCudf.ml"
    "opamHeuristic.ml"
    "opamSolver.ml"
  ]

  requires = [
    "dose3.common"
    "dose3.algo"
    "opam-core"
    "opam-format"
  ]

end
}
